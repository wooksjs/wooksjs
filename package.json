{
  "name": "wooksjs",
  "version": "0.4.10",
  "description": "wooksjs - web app framework with hooks",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap",
    "build": "node ./scripts/build",
    "release": "node ./scripts/release",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts packages/**/src/**/**.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "docs": "vitepress dev docs",
    "docs-build": "vitepress build docs",
    "docs-serve": "vitepress serve docs",
    "jiti": "node ./scripts/jiti.js",
    "jiti:dev": "node ./scripts/jiti.js --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooksjs/wooksjs.git"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "keywords": [
    "wooks",
    "wooksjs",
    "web",
    "application",
    "framework",
    "http",
    "rest",
    "restful",
    "server"
  ],
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wooksjs/wooksjs/issues"
  },
  "homepage": "https://github.com/wooksjs/wooksjs#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "@prostojs/dye": "^0.3.0",
    "@prostojs/logger": "^0.3.7",
    "@prostojs/router": "^0.2.1",
    "@prostojs/wf": "^0.0.10",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@types/minimist": "^1.2.5",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.0.0",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "execa": "^5.1.1",
    "jest": "^29.7.0",
    "jiti": "^1.16.0",
    "minimist": "^1.2.6",
    "node-fetch-native": "^1.0.1",
    "rollup": "^4.9.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "semver": "^7.3.8",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0-rc.31",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@prostojs/cli-help": "^0.0.10"
  }
}

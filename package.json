{
  "name": "wooksjs",
  "version": "0.5.25",
  "description": "wooksjs - web app framework with hooks",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "oxlint -c .oxlintrc.json && eslint --ext .ts packages/**/src/**/**.ts",
    "docs": "vitepress dev docs --host \"0.0.0.0\"",
    "docs-build": "vitepress build docs",
    "docs-serve": "vitepress serve docs --host \"0.0.0.0\"",
    "build": "node ./scripts/build.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "pnpm changelog && git add CHANGELOG.md && git commit -m 'chore: update changelog'",
    "release": "pnpm build && pnpm lint && pnpm test && node ./scripts/versions && pnpm publish -r --access public",
    "sync": "node ./scripts/versions",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooksjs/wooksjs.git"
  },
  "keywords": [
    "wooks",
    "wooksjs",
    "web",
    "application",
    "framework",
    "http",
    "rest",
    "restful",
    "server"
  ],
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wooksjs/wooksjs/issues"
  },
  "homepage": "https://github.com/wooksjs/wooksjs#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@prostojs/dye": "^0.4.9",
    "@prostojs/logger": "^0.4.3",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "8.55.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-array-func": "4.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-oxlint": "^0.15.6",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "2.5.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-yml": "1.10.0",
    "husky": "^9.1.7",
    "inquirer": "^12.3.2",
    "oxlint": "^0.15.6",
    "prettier": "3.1.1",
    "rolldown": "1.0.0-beta.1",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.7.3",
    "unplugin-isolated-decl": "^0.10.5",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.8",
    "zx": "^8.3.0"
  }
}
